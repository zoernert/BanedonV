{
  "name": "banedonv",
  "version": "1.0.0",
  "description": "BanedonV - Advanced File Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run dev:both",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend",
    "build:backend": "tsc && cp -r src/public dist/",
    "start": "node dist/server.js",
    "deploy:dev": "./scripts/deploy-dev.sh",
    "deploy:prod": "./scripts/deploy-prod.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "seed": "ts-node src/data/seeders/index.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js"
  },
  "keywords": [
    "file-management",
    "search",
    "organization",
    "typescript",
    "node",
    "react",
    "next.js"
  ],
  "author": "BanedonV Team",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "faker": "^5.5.3",
    "helmet": "^6.1.5",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/faker": "^5.5.8",
    "@types/jest": "^29.5.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.23",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
